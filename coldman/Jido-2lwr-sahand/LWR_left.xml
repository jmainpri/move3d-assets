    <transformation>
      <translation> 0. 0.06 0 </translation>  
    </transformation>    
    <collision>  no  </collision>
    <display>  no  </display>
    <color>  1.0 1.0 0.0 1.0 </color> 
  </body>   


         <joint name="Joint3">
