             <joint name="Joint6">
               <name> Joint6 </name>
               <type> revolute </type>
               <transformation>
                  <YPR_angles> 0 0 90 </YPR_angles>     
               </transformation>   
               <qmin> -130 </qmin>
               <qmax>  130 </qmax> 

               <body name="kuka6">
