<!-- Kuka LWR arm model -->

<robot name="LWR">
<name> LWR </name>
<models_directory> ./LWR </models_directory>
<joint name="root">
  <name> rootKuka </name>
  <type> revolute </type>
  <transformation>
    <translation> 0 0 0.0 </translation>
  </transformation>  
  <qmin> 0  </qmin>
  <qmax> 0  </qmax>
  <q> 0 </q>       
  
  <body name="base">
    <name> kukaBase  </name> 
    <collision>  yes  </collision>
    <display>  yes  </display>
      <file> base.obj  </file>
      <transformation>
        <translation> 0 0 0.3105 </translation>
      </transformation>    
      <color> 0.7 0.87 0.88 1.0 </color> 
  </body>  
  <body name="GBase">
    <name> GkukaBase  </name> 
    <box> 0.16 0.16 0.12  </box>
    <transformation>
      <translation> 0 0 0.07 </translation>  
    </transformation>    
    <collision>  no  </collision>
    <display>  no  </display>
    <color>  1.0 1.0 0.0 1.0 </color> 
  </body>   


 
  <joint name="Joint1">
    <name> Joint1 </name>
      <type> revolute </type>
      <transformation>
         <translation> 0 0 0.3105 </translation>
      </transformation>         
      <qmin> -170 </qmin>
      <qmax> 170 </qmax>  

      <body name="kuka1">
        <name> kuka1  </name> 
    <collision>  yes  </collision>
    <display>  yes  </display>
        <file> kuka1.obj  </file>
        <color> 1.0 0.3922 0.0 1.0 </color>
      </body>  

  <body name="GKuka1">
    <name> GKuka1  </name> 
    <box> 0.16 0.23 0.26  </box>
    <transformation>
      <translation> 0 0 -0.06 </translation>  
    </transformation>    
    <collision>  no  </collision>
    <display>  no  </display>
    <color>  1.0 1.0 0.0 1.0 </color> 
  </body>   


      <joint name="Joint2">
         <name> Joint2 </name>
         <type> revolute </type>
         <transformation>
            <YPR_angles> 0 0 90 </YPR_angles>     
         </transformation>   
         <qmin> -120 </qmin>
         <qmax>  120 </qmax> 

         <body name="kuka2">
           <name> kuka2  </name> 
    <collision>  yes  </collision>
    <display>  yes  </display>
