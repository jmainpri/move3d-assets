         <type> revolute </type>
         <transformation>
            <translation> 0 0.4 0 </translation>
            <YPR_angles> 0 0 -90 </YPR_angles>     
         </transformation>   
         <qmin> -170 </qmin>
         <qmax>  170 </qmax> 

